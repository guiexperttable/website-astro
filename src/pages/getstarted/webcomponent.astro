import Header from "../../components/header.astro";
import Webcomponent from "../../components/getstarted/webcomponent.md";
import GetStartedSection from "../../components/getstarted/common/get-started-section.astro";

const title = 'Get stated (web component)';
---
<IndexLayout title={title}>
